<?xml version="1.0" ?>
<schema xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:framework:Setup/Declaration/Schema/etc/schema.xsd">
	<table name="adesh_banner_banner" resource="default" engine="innodb" comment="adesh_banner_banner Table">
		<column xsi:type="int" name="entity_id" padding="10" unsigned="true" nullable="false" identity="true" comment="Entity Id"/>
		<constraint xsi:type="primary" referenceId="PRIMARY">
			<column name="entity_id"/>
		</constraint>
		<column name="banner_name" nullable="true" xsi:type="text" comment="banner_name"/>
		<column name="age_group" nullable="true" xsi:type="text" comment="age_group"/>
		<column name="image" nullable="true" xsi:type="text" comment="image"/>
	</table>
</schema>
